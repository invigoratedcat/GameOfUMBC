class Position {
  private float x;
  private float y;

  /**
  * Represents a position in 2D space using X and Y
  */
  public Position(float x, float y) {
    this.x=x;
    this.y=y;
  }

  //returns the x value
  public float getX() {return x;}

  //returns the y value
  public float getY() {return y;}
}
